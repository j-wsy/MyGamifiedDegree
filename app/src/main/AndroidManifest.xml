<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.juzza.mygamifieddegree">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="MyGamifiedDegree"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Avatar"></activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".OverWorld" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CourseOverview" />
        <activity android:name=".CourseOverviewY2" />
        <activity android:name=".CourseOverviewY3" />
        <activity android:name=".RewardBoardNew" />
    </application>

</manifest>